---
import Layout from '../../layouts/Layout.astro';
import GatoradeLogo from '../../icons/GatoradeLogo.astro'

---

<Layout title="Welcome to Astro.">
	<main class="w-full min-h-screen grid grid-cols-1 justify-items-center gap-10">

    <!-- hero -->
		<section class="h-[80vh] flex items-center">
      <h1 class="text-transparent p-4 text-6xl bg-clip-text font-black bg-gradient-to-r font-neon-uno er-fondo-verde neon-verde font-pepsi-extended">Packaging</h1>
    </section>

    <!-- Expanding Gatoradr -->
    <section class="w-4/5 text-white mb-20">
      <div class="flex items-center justify-between mb-8">
        <img src="/images/packs/gatorade-neon-logo.svg" alt="Logo Pepsi Neon" class="h-20">
        <h2 class="text-transparent p-4 text-3xl bg-clip-text bg-gradient-to-r font-neon-uno er-fondo-naranja neon-naranja">
          Expanding Gatorlit <br> Beyond México
        </h2>
      </div>
      <div>
        <div class="flex items-center gap-2">
          <img src="/images/packs/logo-cbc.webp" alt="Logo cbc" class="w-20">
          <img src="/images/banderas-redondas/peruflag.webp" alt="Bandera de Perú" class="w-10">
